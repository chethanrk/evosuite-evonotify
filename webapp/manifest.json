{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.evorait.evonotify",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/evora/EL_NOTIFICATION_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotations"
					]
				}
			},
			"annotations": {
				"uri": "annotations/annotations.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "annotations/annotations.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.evorait.evonotify.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.evorait.evonotify.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": false
			},
			"StatusVH": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/StatusVH.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.evorait.evonotify.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": [
						"notFound"
					]
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "worklist",
					"target": [
						"worklist"
					]
				},
				{
					"pattern": "Notification/{objectId}",
					"name": "object",
					"target": [
						"object"
					]
				},
				{
					"pattern": "Notification/{objectId}/Item/{itemId}",
					"name": "item",
					"target": [
						"item"
					]
				},
				{
					"pattern": "Notification/{objectId}/Item/{itemId}/Task/{taskId}",
					"name": "task",
					"target": [
						"task"
					]
				},
				{
					"pattern": "Notification/{objectId}/Item/{itemId}/Activity/{activityId}",
					"name": "activity",
					"target": [
						"activity"
					]
				},
				{
					"pattern": "Notification/{objectId}/Item/{itemId}/Cause/{causeId}",
					"name": "cause",
					"target": [
						"cause"
					]
				}
			],
			"targets": {
				"worklist": {
					"viewName": "Worklist",
					"viewId": "worklist",
					"viewLevel": 1
				},
				"object": {
					"viewName": "Object",
					"viewId": "object",
					"viewLevel": 2
				},
				"item": {
					"viewName": "ObjectItem",
					"viewId": "item",
					"viewLevel": 3
				},
				"task": {
					"viewName": "ObjectTask",
					"viewId": "task",
					"viewLevel": 3
				},
				"activity": {
					"viewName": "ObjectActivity",
					"viewId": "activity",
					"viewLevel": 3
				},
				"cause": {
					"viewName": "ObjectCause",
					"viewId": "cause",
					"viewLevel": 4
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	}
}