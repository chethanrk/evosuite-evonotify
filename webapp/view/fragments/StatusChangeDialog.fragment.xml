<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>tit.selectStatus}"
		confirm="onPressClose"
		cancel="onPressClose"
		items="{
			path: 'StatusVH>/selectedNode'
		}" >
		<StandardListItem
			title="{StatusVH>StatusDescription}"
			description="{StatusVH>StatusCode}"
			type="Inactive" />
	</SelectDialog>
</core:FragmentDefinition>