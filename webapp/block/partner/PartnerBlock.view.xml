<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable"
	controllerName="com.evorait.evosuite.evonotify.block.partner.PartnerBlockController">
	<smartTable:SmartTable height="100%" width="100%" header="{i18n>tit.partners}" tableType="ResponsiveTable" useExportToExcel="false"
		useVariantManagement="false" useTablePersonalisation="false" showRowCount="true"
		persistencyKey="com.evorait.evosuite.evonotify.PartnersTable" enableAutoBinding="true" demandPopin="true"
		entitySet="PMNotificationPartnerSet" beforeRebindTable="onBeforeRebindTable" requestAtLeastFields=""
		tableBindingPath="NotificationToPartner">
		<!-- layout data used to make the table growing but the filter bar fixed -->
		<smartTable:layoutData>
			<FlexItemData growFactor="1" baseSize="0%"/>
		</smartTable:layoutData>
		<Table growing="true" id="idNotificationPartnerTable"></Table>
	</smartTable:SmartTable>
</mvc:View>