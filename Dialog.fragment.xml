<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Status Found"
		title="Select Status"
		showSearch="false"
		search="handleSearch"
		confirm="handleClose"
		close="handleClose"
		items="{
			path: '/SystemStatus'
		}" >
		<StandardListItem
			title="{StatusDescription}"
			description="{StatusCode}"
			type="Inactive" />
	</SelectDialog>
</core:FragmentDefinition>