{
	"_version": "1.4.0",
	"sap.app": {
		"id": "com.evorait.evolite.evonotify",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "mobile-ad74c0790.hana.ondemand.com/com.evorait.evolite.evonotify/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": ["annotations"]
				}
			},
			"annotations": {
				"uri": "annotations/annotations.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "annotations/annotations.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.38.3"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.evorait.evolite.evonotify.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.40.0",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.40.0"
				},
				"sap.m": {
					"minVersion": "1.40.0"
				},
				"sap.uxap": {
					"minVersion": ""
				},
				"sap.ui.comp": {
					"minVersion": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.evorait.evolite.evonotify.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.evorait.evolite.evonotify.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "worklist",
				"target": ["worklist"]
			}, {
				"pattern": "PMNotifications/{objectId}",
				"name": "object",
				"target": ["object"]
			}, {
				"pattern": "PMNotifications/PMNotificationItems/{objectId}/{itemId}",
				"name": "item",
				"target": ["item"]
			}, {
				"pattern": "PMNotifications/PMNotificationTasks/{objectId}/{itemId}/{taskId}",
				"name": "task",
				"target": ["task"]
			}, {
				"pattern": "PMNotifications/PMNotificationActivities/{objectId}/{itemId}/{activityId}",
				"name": "activity",
				"target": ["activity"]
			}, {
				"pattern": "PMNotifications/PMNotificationCauses/{objectId}/{itemId}/{causeId}",
				"name": "cause",
				"target": ["cause"]
			}],
			"targets": {
				"worklist": {
					"viewName": "Worklist",
					"viewId": "worklist",
					"viewLevel": 1
				},
				"object": {
					"viewName": "Object",
					"viewId": "object",
					"viewLevel": 2
				},
				"item": {
					"viewName": "ObjectItem",
					"viewId": "item",
					"viewLevel": 3
				},
				"task": {
					"viewName": "ObjectTask",
					"viewId": "task",
					"viewLevel": 3
				},
				"activity": {
					"viewName": "ObjectActivity",
					"viewId": "activity",
					"viewLevel": 3
				},
				"cause": {
					"viewName": "ObjectCause",
					"viewId": "cause",
					"viewLevel": 4
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "",
		"_version": "1.1.0"
	}
}